#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/fres/dev/projects/vitalis/node_modules/.pnpm/svelte-check@3.8.1_postcss-load-config@4.0.2_postcss@8.4.38__postcss@8.4.38_svelte@5.0.0-next.164/node_modules/svelte-check/bin/node_modules:/Users/fres/dev/projects/vitalis/node_modules/.pnpm/svelte-check@3.8.1_postcss-load-config@4.0.2_postcss@8.4.38__postcss@8.4.38_svelte@5.0.0-next.164/node_modules/svelte-check/node_modules:/Users/fres/dev/projects/vitalis/node_modules/.pnpm/svelte-check@3.8.1_postcss-load-config@4.0.2_postcss@8.4.38__postcss@8.4.38_svelte@5.0.0-next.164/node_modules:/Users/fres/dev/projects/vitalis/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/fres/dev/projects/vitalis/node_modules/.pnpm/svelte-check@3.8.1_postcss-load-config@4.0.2_postcss@8.4.38__postcss@8.4.38_svelte@5.0.0-next.164/node_modules/svelte-check/bin/node_modules:/Users/fres/dev/projects/vitalis/node_modules/.pnpm/svelte-check@3.8.1_postcss-load-config@4.0.2_postcss@8.4.38__postcss@8.4.38_svelte@5.0.0-next.164/node_modules/svelte-check/node_modules:/Users/fres/dev/projects/vitalis/node_modules/.pnpm/svelte-check@3.8.1_postcss-load-config@4.0.2_postcss@8.4.38__postcss@8.4.38_svelte@5.0.0-next.164/node_modules:/Users/fres/dev/projects/vitalis/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../svelte-check/bin/svelte-check" "$@"
else
  exec node  "$basedir/../svelte-check/bin/svelte-check" "$@"
fi
